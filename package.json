{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Get started with Webpack!",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "dev": "better-npm-run dev",
    "start": "better-npm-run dev",
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node ./node_modules/webpack/bin/webpack",
      "env": { "DEBUG": "app:*" }
    },
    "dev": {
      "command": "nodemon --exec babel-node src/server",
      "env": { "NODE_ENV": "development", "DEBUG": "app:*" }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": { "NODE_ENV": "production", "DEBUG": "app:*" }
    },
    "test": {
      "command": "karma start",
      "env": { "NODE_ENV": "test", "BABEL_ENV": "test", "DEBUG": "app:*" }
    },
    "test:watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": { "NODE_ENV": "test", "BABEL_ENV": "test", "DEBUG": "app:*" }
    }
  },
  "keywords": [],
  "author": "Henry Cruz",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.9",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.7",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.2.0",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
